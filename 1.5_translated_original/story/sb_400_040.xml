    <Entry>
      <PointerOffset/>
      <JapaneseText>&lt;Dio&gt;</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
