      <JapaneseText>お兄ちゃん好きの男の子</JapaneseText>
      <EnglishText>Little Kid</EnglishText>
      <Notes/>
      <Id>1</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>悲しみを背負った母親</JapaneseText>
      <EnglishText>Sad Mother</EnglishText>
      <Notes/>
      <Id>2</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>Variable</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
