      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>29</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>268024</PointerOffset>
      <JapaneseText>だが、
