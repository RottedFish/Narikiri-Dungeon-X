      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>23</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266420</PointerOffset>
      <JapaneseText>エメラルドで、
