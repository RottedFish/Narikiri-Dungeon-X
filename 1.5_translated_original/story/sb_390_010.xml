      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265264</PointerOffset>
      <JapaneseText>モリスン……どこかで
