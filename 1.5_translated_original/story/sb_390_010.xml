      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261964</PointerOffset>
      <JapaneseText>時間転移術って……</JapaneseText>
