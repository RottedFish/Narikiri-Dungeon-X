      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265628</PointerOffset>
      <JapaneseText>どこで？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265936</PointerOffset>
      <JapaneseText>さぁ……なんだったっけ……
