      <SpeakerId>2</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262324</PointerOffset>
      <JapaneseText>どうした？</JapaneseText>
