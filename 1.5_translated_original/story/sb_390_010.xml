      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>22</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266280</PointerOffset>
      <JapaneseText>それでその本、
他にどんなことが
