    <Entry>
      <PointerOffset/>
      <JapaneseText>ダオス</JapaneseText>
      <EnglishText>Dhaos</EnglishText>
      <Notes/>
      <Id>4</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
