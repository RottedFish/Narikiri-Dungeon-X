      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>260804</PointerOffset>
      <VoiceId>VSM_260_020_005</VoiceId>
      <JapaneseText>別の方向にも
違うもんが見えたぞ</JapaneseText>
      <EnglishText>&lt;Mel&gt; please don't start copying &lt;Dio&gt;. </EnglishText>
      <Notes/>
      <SpeakerId>1</SpeakerId>
