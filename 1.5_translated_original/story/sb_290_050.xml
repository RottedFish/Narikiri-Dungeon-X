    <Entry>
      <PointerOffset>278268</PointerOffset>
      <JapaneseText>クルール！　クルール！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>278408</PointerOffset>
      <JapaneseText>来たのか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>278996</PointerOffset>
      <JapaneseText>うわっ、しゃべった……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>279136</PointerOffset>
      <JapaneseText>なぜ、人間まで連れてきた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>279332</PointerOffset>
      <JapaneseText>クルール！　クルール！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>279800</PointerOffset>
      <JapaneseText>おまえ……何なんだ！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>279940</PointerOffset>
      <JapaneseText>私は『懇情のひとつぶ』</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280472</PointerOffset>
      <JapaneseText>コンジョウのひと……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280668</PointerOffset>
      <JapaneseText>この星に根を張り、マナを
養分にして、大輪の花を
咲かせ、大いなる実りをなす</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280864</PointerOffset>
      <JapaneseText>養分にするって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>281420</PointerOffset>
      <JapaneseText>クルール！　クルール！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>281840</PointerOffset>
      <JapaneseText>星全体のマナを
吸い尽くすつもりだよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282572</PointerOffset>
      <JapaneseText>そんなことしたら、
この星が死んじゃう……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282712</PointerOffset>
      <JapaneseText>この星のことなんて
知らない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282712</PointerOffset>
      <JapaneseText>なした実りを再び
空に届け、宙を巡り、
新たな養分となる星を探す</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>14</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283484</PointerOffset>
      <JapaneseText>空にって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283484</PointerOffset>
      <JapaneseText>まさか、あん時、
空から降ってきたの
おまえ……！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>15</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283744</PointerOffset>
      <JapaneseText>私たちの繁栄の営みを
邪魔をする者は排除するのみ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283940</PointerOffset>
      <JapaneseText>排除って、戦うの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284136</PointerOffset>
      <JapaneseText>クルールゥ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284768</PointerOffset>
      <JapaneseText>あんたも
敵に回るというなら、
容赦はしない！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285456</PointerOffset>
      <JapaneseText>あ、あいつ！
まだやられてなかったんだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285756</PointerOffset>
      <JapaneseText>……やらなきゃ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>286120</PointerOffset>
      <JapaneseText>ああ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>22</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
