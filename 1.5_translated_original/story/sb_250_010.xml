  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>259732</PointerOffset>
      <JapaneseText>……ここらで少し休憩しないか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260240</PointerOffset>
      <JapaneseText>そうしようか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260380</PointerOffset>
      <JapaneseText>それじゃあ、オレは
辺りをぶらぶらしようかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260620</PointerOffset>
      <JapaneseText>わたしたちは、
買い物に行こうよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262728</PointerOffset>
      <JapaneseText>それじゃあ、僕は
ちょっと剣の特訓でも
してるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263292</PointerOffset>
      <JapaneseText>え……特訓……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263712</PointerOffset>
      <JapaneseText>おいおい、
休憩の意味なくないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263852</PointerOffset>
      <JapaneseText>そうかもしれないけど、
なんか落ち着かなくてさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264792</PointerOffset>
      <JapaneseText>&lt;Cless&gt;さん、
オレも一緒に……
いいかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265108</PointerOffset>
      <JapaneseText>おい、&lt;Dio&gt;、
やめといたほうがいいぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265108</PointerOffset>
      <JapaneseText>そいつの特訓は
ハンパじゃないからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265732</PointerOffset>
      <JapaneseText>そう、かな？
確かにそう易しくは
ないけどね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266208</PointerOffset>
      <JapaneseText>ふっふっふ！
オレだってここまで
旅を続けてきたんだぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266208</PointerOffset>
      <JapaneseText>それなりに
強くなってるしな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>12</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266760</PointerOffset>
      <JapaneseText>よし、それじゃあ、
特別に君に新しい技を
伝授するよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>266760</PointerOffset>
      <JapaneseText>まずランニングだ。
ついてこられるかな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>13</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>267516</PointerOffset>
      <JapaneseText>望むところだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>268168</PointerOffset>
      <JapaneseText>あ、おい、ちょっ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>268168</PointerOffset>
      <JapaneseText>……知らねぇぞ……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>15</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
</SceneText>
