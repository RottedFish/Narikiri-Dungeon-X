      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>32</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265848</PointerOffset>
