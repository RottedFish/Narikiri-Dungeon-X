      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263820</PointerOffset>
      <JapaneseText>初々しい！　多くは
