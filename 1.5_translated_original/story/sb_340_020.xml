      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>22</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264288</PointerOffset>
      <JapaneseText>いや！　だからこそ、
