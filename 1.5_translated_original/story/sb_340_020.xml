      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262464</PointerOffset>
      <JapaneseText>誰だ、こんなの入れたのは！
