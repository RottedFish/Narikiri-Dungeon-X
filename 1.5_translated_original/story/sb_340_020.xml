      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>24</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264476</PointerOffset>
