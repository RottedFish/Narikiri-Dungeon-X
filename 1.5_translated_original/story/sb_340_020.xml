      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>30</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265552</PointerOffset>
