      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264008</PointerOffset>
      <JapaneseText>活きはいいが、
