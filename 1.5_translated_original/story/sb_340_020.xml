      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>31</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265692</PointerOffset>
