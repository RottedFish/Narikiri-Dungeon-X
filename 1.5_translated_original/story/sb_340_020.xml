      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263072</PointerOffset>
      <JapaneseText>この闘技場でも
