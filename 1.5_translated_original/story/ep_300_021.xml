  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>259056</PointerOffset>
      <VoiceId>VSM_300_020_001</VoiceId>
      <JapaneseText>……にしても、派手に
やっちゃったんだね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259376</PointerOffset>
      <VoiceId>VSM_300_020_002</VoiceId>
      <JapaneseText>魔科学兵器は
デリス・カーラーン全体の
マナを吸い取ったけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259376</PointerOffset>
      <VoiceId>VSM_300_020_003</VoiceId>
      <JapaneseText>ここは直撃を受けた場所だから、
特にひどいんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259816</PointerOffset>
      <VoiceId>VSM_300_020_004</VoiceId>
      <JapaneseText>どうして、
きみがそんなこと
知ってるの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260116</PointerOffset>
      <VoiceId>VSM_300_020_005</VoiceId>
      <JapaneseText>え……なんでかな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260116</PointerOffset>
      <VoiceId>VSM_300_020_006</VoiceId>
      <JapaneseText>なんかぼんやりと
いろんなことが
頭に流れ込んでて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260576</PointerOffset>
      <VoiceId>VSM_300_020_007</VoiceId>
      <JapaneseText>オレもなんかそんな感じ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261056</PointerOffset>
      <VoiceId>VSM_300_020_008</VoiceId>
      <JapaneseText>………………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261412</PointerOffset>
      <VoiceId>VSM_300_020_009</VoiceId>
      <JapaneseText>故郷がこんなだもんね……
そりゃ必死にもなるか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261412</PointerOffset>
      <VoiceId>VSM_300_020_010</VoiceId>
      <JapaneseText>どうして、あの時……
気付けなかったんだろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262904</PointerOffset>
      <VoiceId>VSM_300_020_012</VoiceId>
      <JapaneseText>これって……もしかして……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263044</PointerOffset>
      <VoiceId>VSM_300_020_013</VoiceId>
      <JapaneseText>ん……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263184</PointerOffset>
      <VoiceId>VSM_300_020_014</VoiceId>
      <JapaneseText>私の誕生日、
なんで知ってるの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263324</PointerOffset>
      <VoiceId>VSM_300_020_015</VoiceId>
      <JapaneseText>誕生日なのか……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263464</PointerOffset>
      <VoiceId>VSM_300_020_016</VoiceId>
      <JapaneseText>え？　プレゼントじゃないの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263604</PointerOffset>
      <VoiceId>VSM_300_020_017</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263744</PointerOffset>
      <VoiceId>VSM_300_020_018</VoiceId>
      <JapaneseText>……あはっ、
なんでもいいや。
ありがと、ダオス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263884</PointerOffset>
      <VoiceId>VSM_300_020_019</VoiceId>
      <JapaneseText>いや……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263884</PointerOffset>
      <VoiceId>VSM_300_020_020</VoiceId>
      <JapaneseText>……絶対に、
なくすな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>15</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264024</PointerOffset>
      <VoiceId>VSM_300_020_021</VoiceId>
      <JapaneseText>もちろん！　大事にするよ！
だって……ダオスがくれた
ものだもん！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264164</PointerOffset>
      <VoiceId>VSM_300_020_022</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265092</PointerOffset>
      <VoiceId>VSM_300_020_023</VoiceId>
      <JapaneseText>あの日以来、あいつを
森の中で見ることはなくなった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265092</PointerOffset>
      <VoiceId>VSM_300_020_024</VoiceId>
      <JapaneseText>思えばあれは
餞別だったのかもしれない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265092</PointerOffset>
      <VoiceId>VSM_300_020_025</VoiceId>
      <JapaneseText>ちゃんとあの時、気付いて、
止められたらよかったのに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265092</PointerOffset>
      <VoiceId>VSM_300_020_026</VoiceId>
      <JapaneseText>せめて……
彼の想いを……気持ちを
ちゃんと確かめられれば……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>4</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
</SceneText>
