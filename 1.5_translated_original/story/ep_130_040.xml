    <Entry>
      <PointerOffset>264160</PointerOffset>
      <VoiceId>VSM_130_040_010</VoiceId>
      <JapaneseText>だぁあっ！</JapaneseText>
      <EnglishText>Take this! </EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
