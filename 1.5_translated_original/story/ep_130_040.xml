      <SpeakerId>4</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>263400</PointerOffset>
      <VoiceId>VSM_130_040_009</VoiceId>
      <JapaneseText>ボロボロにしてやる</JapaneseText>
      <EnglishText>Let's mess him bad. </EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
