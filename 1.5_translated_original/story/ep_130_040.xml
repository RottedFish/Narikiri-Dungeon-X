    <Entry>
      <PointerOffset>261036</PointerOffset>
      <VoiceId>VSM_130_040_003</VoiceId>
      <JapaneseText>&lt;Cless&gt;さんの代わりだとか</JapaneseText>
      <EnglishText>He is replacing Mister &lt;Cless&gt; on duty. </EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>261748</PointerOffset>
      <VoiceId>VSM_130_040_005</VoiceId>
      <JapaneseText>おい、おまえ！</JapaneseText>
      <EnglishText>Hey, you! </EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
