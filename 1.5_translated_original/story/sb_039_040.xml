    <Entry>
      <PointerOffset/>
      <JapaneseText>ワンダーシェフ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
