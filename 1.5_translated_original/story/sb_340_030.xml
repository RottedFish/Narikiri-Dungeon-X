      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261564</PointerOffset>
      <JapaneseText>逃げたりなんてしません！</JapaneseText>
