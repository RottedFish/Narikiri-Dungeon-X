      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>2</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261096</PointerOffset>
      <JapaneseText>オレだって、
