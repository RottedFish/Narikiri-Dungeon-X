  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>258608</PointerOffset>
      <JapaneseText>おーおーおー、
おめぇら、待たせただ。
ちーっと、ヤボ用があっただーよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259056</PointerOffset>
      <JapaneseText>ううん、大丈夫ですよ、
アルベルトさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259196</PointerOffset>
      <JapaneseText>ソ。
平気、平気！
みーんな待ってたから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259336</PointerOffset>
      <JapaneseText>クルール♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259532</PointerOffset>
      <JapaneseText>なーんか、気持ち悪い空気だよ。
おい！　ハゲぼうず！
こりゃ、どういうことだ!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259728</PointerOffset>
      <JapaneseText>…………、
し、知らねぇよ。
お前が何してたかなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260348</PointerOffset>
      <JapaneseText>シー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260592</PointerOffset>
      <JapaneseText>もう、&lt;Dio&gt;！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260788</PointerOffset>
      <JapaneseText>や、やべっ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260984</PointerOffset>
      <JapaneseText>あーん？
なんだかよくわらんが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260984</PointerOffset>
      <JapaneseText>おらは、ちっと運動したもんで
喉を潤しに馴染みの店に
行っただよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260984</PointerOffset>
      <JapaneseText>それが、どうかしただか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261792</PointerOffset>
      <JapaneseText>ふーん……、
運動をしたかラ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261932</PointerOffset>
      <JapaneseText>馴染みのお店で……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262072</PointerOffset>
      <JapaneseText>クルール！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262212</PointerOffset>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262660</PointerOffset>
      <JapaneseText>ま、じゃ、そういう事で、
いいんじゃないかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262800</PointerOffset>
      <JapaneseText>うんうん、そうだね！
よし、行こう！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262940</PointerOffset>
      <JapaneseText>クッルール！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263788</PointerOffset>
      <JapaneseText>なーんか、わっかんねぇが
損はしてねぇようだから
放っておくだかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264608</PointerOffset>
      <JapaneseText>んなもん、いらんだーよ！
おらはおらで、おらだけだ！
そんなもんに縛られないだーよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>265040</PointerOffset>
      <JapaneseText>へっ！
わかりゃーいいだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
