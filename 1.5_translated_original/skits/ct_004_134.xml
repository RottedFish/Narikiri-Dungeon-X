    <Entry>
      <PointerOffset>37244</PointerOffset>
      <VoiceId>VCT_004_134_001</VoiceId>
      <JapaneseText>&lt;Mel&gt;&lt;Mel&gt;〜！　すんばらしい、すんばらしい一撃だど！
おら、惚れ直しただ、猛烈に惚れ直しただー∀</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39468</PointerOffset>
      <VoiceId>VCT_004_134_002</VoiceId>
      <JapaneseText>え、あ、ありがとうございます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40964</PointerOffset>
      <VoiceId>VCT_004_134_003</VoiceId>
      <JapaneseText>つーか、よくあんなむずいのできんのな。
やるの大変じゃねーか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41196</PointerOffset>
      <VoiceId>VCT_004_134_004</VoiceId>
      <JapaneseText>そう、かな……
そうでもない、と思うけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41556</PointerOffset>
      <VoiceId>VCT_004_134_005</VoiceId>
      <JapaneseText>なんかコツとかあんのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41916</PointerOffset>
      <VoiceId>VCT_004_134_006</VoiceId>
      <JapaneseText>あんま難しく考えないで、
ガチャガチャってやればいいんだと思う、うん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42592</PointerOffset>
      <VoiceId>VCT_004_134_007</VoiceId>
      <JapaneseText>ガチャガチャ？　なんだ、そりゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42984</PointerOffset>
      <VoiceId>VCT_004_134_008</VoiceId>
      <JapaneseText>ガチャガチャって感じ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43296</PointerOffset>
      <VoiceId>VCT_004_134_009</VoiceId>
      <JapaneseText>よくわかんね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43464</PointerOffset>
      <VoiceId>VCT_004_134_010</VoiceId>
      <JapaneseText>ガチャガチャっつーのはな、こう、スーっときて
ダーッときてポンって事だ！　な〜、&lt;Mel&gt;&lt;Mel&gt;</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44992</PointerOffset>
      <VoiceId>VCT_004_134_011</VoiceId>
      <JapaneseText>てめぇもわかってねーだろ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
