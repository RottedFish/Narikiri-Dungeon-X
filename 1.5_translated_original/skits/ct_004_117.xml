    <Entry>
      <PointerOffset>37120</PointerOffset>
      <VoiceId>VCT_004_117_001</VoiceId>
      <JapaneseText>か、勝てね〜……んだよ、アイツ……
インチキだろ、あの強さ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
