  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>37120</PointerOffset>
      <VoiceId>VCT_002_048_001</VoiceId>
      <JapaneseText>さっき&lt;Dio&gt;くんが着ていた衣装……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37360</PointerOffset>
      <VoiceId>VCT_002_048_002</VoiceId>
      <JapaneseText>&lt;Chester&gt;さんのお家の整理を
お手伝いしてる時に見たものに似てるんですが……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37528</PointerOffset>
      <VoiceId>VCT_002_048_003</VoiceId>
      <JapaneseText>きっと、&lt;Chester&gt;が
彼にあげたんだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37892</PointerOffset>
      <VoiceId>VCT_002_048_004</VoiceId>
      <JapaneseText>あれは……&lt;Chester&gt;が
アミィのために作ったものだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38204</PointerOffset>
      <VoiceId>VCT_002_048_005</VoiceId>
      <JapaneseText>&lt;Chester&gt;さん……あの子たちと
アミィさんを重ねて見ていたのでしょうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38552</PointerOffset>
      <VoiceId>VCT_002_048_006</VoiceId>
      <JapaneseText>……あいつはアミィの死から
解放されるべきだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39176</PointerOffset>
      <VoiceId>VCT_002_048_007</VoiceId>
      <JapaneseText>後ろを振り返り続けてたら、
アミィも喜ばないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39612</PointerOffset>
      <VoiceId>VCT_002_048_008</VoiceId>
      <JapaneseText>……本人にそう言ってあげないんですか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39972</PointerOffset>
      <VoiceId>VCT_002_048_009</VoiceId>
      <JapaneseText>そうだね。僕が言ってやらないと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40084</PointerOffset>
      <VoiceId>VCT_002_048_010</VoiceId>
      <JapaneseText>………………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40468</PointerOffset>
      <VoiceId>VCT_002_048_011</VoiceId>
      <JapaneseText>&lt;Chester&gt;さんを傷つけたくない……
&lt;Cless&gt;さんは優しいんですね。でも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40972</PointerOffset>
      <VoiceId>VCT_002_048_012</VoiceId>
      <JapaneseText>わかってるよ、&lt;Mint&gt;。
正直に言うのも……優しさだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41472</PointerOffset>
      <VoiceId>VCT_002_048_013</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>13</Id>
