  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>37872</PointerOffset>
      <VoiceId>VCT_004_142_001</VoiceId>
      <JapaneseText>ホウキってすごーい。
空飛んでる。気持ちいい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38196</PointerOffset>
      <VoiceId>VCT_004_142_002</VoiceId>
      <JapaneseText>なんだよ。&lt;Mel&gt;も
簡単にホウキに乗れてるじゃねぇか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38520</PointerOffset>
      <VoiceId>VCT_004_142_003</VoiceId>
      <JapaneseText>ちっちっち。
&lt;Mel&gt;ちゃんも素質を持っているのだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39060</PointerOffset>
      <VoiceId>VCT_004_142_004</VoiceId>
      <JapaneseText>ねえ&lt;Mel&gt;ちゃん。ホウキ仲間になったところで
一緒にいいトコロいっちゃう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39668</PointerOffset>
      <VoiceId>VCT_004_142_005</VoiceId>
      <JapaneseText>敵をいっぱい
打ち落としちゃったりして〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39836</PointerOffset>
      <VoiceId>VCT_004_142_006</VoiceId>
      <JapaneseText>楽しそうですね。
でもまだホウキに慣れてなくて、そこまで……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40068</PointerOffset>
      <VoiceId>VCT_004_142_007</VoiceId>
      <JapaneseText>いいから、いいから。
見学だけでも楽しいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40856</PointerOffset>
      <VoiceId>VCT_004_142_008</VoiceId>
      <JapaneseText>ついて行きます！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41092</PointerOffset>
      <VoiceId>VCT_004_142_009</VoiceId>
      <JapaneseText>んじゃ、&lt;Chester&gt;
ちょっとだけ『ぐるぅびぃ』してくるねー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42224</PointerOffset>
      <VoiceId>VCT_004_142_010</VoiceId>
      <JapaneseText>おい、&lt;Arche&gt;どこいくんだよ。
ていうか『ぐるぅびぃ』ってなんだよ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
</SceneText>
