    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>エトス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>Variable</JapaneseText>
