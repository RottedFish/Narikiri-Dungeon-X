    <Entry>
      <PointerOffset>37676</PointerOffset>
      <VoiceId>VCT_001_031_003</VoiceId>
      <JapaneseText>あの、さ……</JapaneseText>
      <EnglishText>Hey, um...</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>37900</PointerOffset>
      <VoiceId>VCT_001_031_004</VoiceId>
      <JapaneseText>なんだよ</JapaneseText>
      <EnglishText>What?</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>38200</PointerOffset>
      <VoiceId>VCT_001_031_005</VoiceId>
      <JapaneseText>……何怒ってんの？</JapaneseText>
      <EnglishText>...Why are you angry, now?</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>38448</PointerOffset>
      <VoiceId>VCT_001_031_006</VoiceId>
      <JapaneseText>別に。怒ってなんかねーって</JapaneseText>
      <EnglishText>Nope. I'm not mad at all.</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>38804</PointerOffset>
      <VoiceId>VCT_001_031_007</VoiceId>
      <JapaneseText>……ゴメン</JapaneseText>
      <EnglishText>...Sorry.</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>38972</PointerOffset>
      <VoiceId>VCT_001_031_008</VoiceId>
      <JapaneseText>怒ってねーってんだろ？</JapaneseText>
      <EnglishText>Are YOU angry?</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>39528</PointerOffset>
      <VoiceId>VCT_001_031_009</VoiceId>
      <JapaneseText>謝ってるのに、何で怒んの？</JapaneseText>
      <EnglishText>I'm apologizing! Why are you angry now?</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>39776</PointerOffset>
      <VoiceId>VCT_001_031_010</VoiceId>
      <JapaneseText>怒ってねーっての！</JapaneseText>
      <EnglishText>I'm NOT angry!</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>40016</PointerOffset>
      <VoiceId>VCT_001_031_011</VoiceId>
      <JapaneseText>じゃあ、笑って</JapaneseText>
      <EnglishText>Then smile.</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>40248</PointerOffset>
      <VoiceId>VCT_001_031_012</VoiceId>
      <JapaneseText>突然言われて、笑えるかって</JapaneseText>
      <EnglishText>Just telling me to smile won't make me smile.</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>40496</PointerOffset>
      <VoiceId>VCT_001_031_013</VoiceId>
      <JapaneseText>やっぱり、怒ってるんだ</JapaneseText>
      <EnglishText>I knew it, you're mad!</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>40664</PointerOffset>
      <VoiceId>VCT_001_031_014</VoiceId>
      <JapaneseText>……んだよ、もう</JapaneseText>
      <EnglishText>Ugh...</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>41568</PointerOffset>
      <VoiceId>VCT_001_031_015</VoiceId>
      <JapaneseText>気持ち悪い</JapaneseText>
      <EnglishText>That's just creepy...</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>42120</PointerOffset>
      <VoiceId>VCT_001_031_016</VoiceId>
      <JapaneseText>んだよ、どうすりゃいいってんだ！？</JapaneseText>
      <EnglishText>God, what do you want me to do!?</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>42668</PointerOffset>
      <VoiceId>VCT_001_031_017</VoiceId>
      <JapaneseText>乙女心は難しいんだよ、&lt;Dio&gt;くん</JapaneseText>
      <EnglishText>A young girl's heart is like a web of complexities, &lt;Dio&gt;!</EnglishText>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>Editing</Status>
