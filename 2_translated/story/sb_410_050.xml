      <JapaneseText>サーウー・ギスキー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>Variable</JapaneseText>
