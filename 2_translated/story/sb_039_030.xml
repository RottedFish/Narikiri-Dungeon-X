    <Entry>
      <PointerOffset>278284</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_001&gt;うぅっ……寒っ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>278728</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_002&gt;どこかで
あったまろうよ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>279428</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_003&gt;きゃっ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280004</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_004&gt;私を呼んでくれて、
サンキュー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280312</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_005&gt;呼んでねーっての……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280576</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_006&gt;ワンダーシェフ！
ここに参上！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280716</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_007&gt;……何しに
出てきたんだよ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280912</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_008&gt;寒く冷たいあなたの心に、
このスープをあげるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280912</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_009&gt;じっくりコトコト
煮込んだ末に、
できたこのスープ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>8</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>280912</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_010&gt;食卓のお供に、毎朝、
笑顔で朝食を
食べられる代物よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>8</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>281816</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_011&gt;んじゃあ、
いただきまーす！
んぐんぐっ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282068</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_012&gt;か、かれぇ〜！！！
……でも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282068</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_013&gt;美味い！
そして……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282068</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_014&gt;体が
ポッカポッカする！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282776</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_015&gt;ポッカポッカ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282972</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_016&gt;そ、ポッカポッカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283348</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_017&gt;なんかカレーの匂いが
するね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>283612</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_018&gt;いや、でも
中華っぽくもあるぜ？
……これ、なんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284032</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_019&gt;それはカレーとマーボー豆腐の
辛さの饗宴、
マーボーカレースープ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284284</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_020&gt;マーボーカレースープ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284480</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_022&gt;辛いものを体内に
取り込むと、発汗作用が
促進されて体が温かくなるの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284480</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_023&gt;だから、
寒い時には最適</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>17</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>284724</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_024&gt;おまえも
食ってみろよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285264</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_025&gt;いただきます。
んぐんぐんぐ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285264</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_026&gt;辛くて、汗が
たくさん出るけど、
美味しいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>19</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285924</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_027&gt;そんだけ汗かいたら、
その分、少しは痩せるぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>288212</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_028&gt;いっ……てぇ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>289508</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_029&gt;わたし、太ってないもん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>22</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>289508</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_030&gt;んぐんぐ……
おいしい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>22</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>290512</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_031&gt;ポッカポッカというより、
ボッカボッカかしら。
あら、私うまいこと言った</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>23</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>290764</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_032&gt;どこが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>24</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>290960</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_033&gt;…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>25</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>290960</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_034&gt;ポッカポッカというより、
ボッカボッカね、えへ∀</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>25</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>291504</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_035&gt;２回言うな！
そして言いっぱなしにして
逃げるな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>26</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>291912</PointerOffset>
      <JapaneseText>&lt;VSS_039_030_036&gt;でも、あの人、
レシピとスープの材料
置いていってくれたよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>27</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
