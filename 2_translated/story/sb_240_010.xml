  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>258948</PointerOffset>
      <JapaneseText>あら……帰ってきたの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259144</PointerOffset>
      <JapaneseText>帰ってきたの、とは何だ。
ここは私の家だ。
帰ってきても構わないだろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259284</PointerOffset>
      <JapaneseText>ダメだとは言ってないわよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259424</PointerOffset>
      <JapaneseText>……まったく、もう……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259668</PointerOffset>
      <JapaneseText>……相変わらず
仲がいいね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259968</PointerOffset>
      <JapaneseText>え……どこが？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260212</PointerOffset>
      <JapaneseText>&lt;Mel&gt;、&lt;Dio&gt;に
言っても……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260576</PointerOffset>
      <JapaneseText>そうだね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261148</PointerOffset>
      <JapaneseText>そうそう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261148</PointerOffset>
      <JapaneseText>あなたの留守中に掃除してたら、
棚の奥から、こんなものが
見つかったわよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261288</PointerOffset>
      <JapaneseText>何だ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261428</PointerOffset>
      <JapaneseText>お父様からの手紙</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261800</PointerOffset>
      <JapaneseText>……捨てとけ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261940</PointerOffset>
      <JapaneseText>ちょっと……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262136</PointerOffset>
      <JapaneseText>どうせろくなこと、
書いてないんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262276</PointerOffset>
      <JapaneseText>あなたに渡したいものが
あるって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262592</PointerOffset>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262592</PointerOffset>
      <JapaneseText>い、いや……
どうせろくなもんじゃない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>16</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262592</PointerOffset>
      <JapaneseText>植物の標本とか、
図鑑とかそんなことだろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>16</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262992</PointerOffset>
      <JapaneseText>本当にもう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>262992</PointerOffset>
      <JapaneseText>……この手紙、
持っていって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>17</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263188</PointerOffset>
      <JapaneseText>え……でも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263328</PointerOffset>
      <JapaneseText>お父さんから&lt;Claus&gt;に宛てた
言葉が書いてあるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263328</PointerOffset>
      <JapaneseText>お父さんの
残したもののありかも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263328</PointerOffset>
      <JapaneseText>機会があったら、こっそり
渡しといてくれればいいわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263328</PointerOffset>
      <JapaneseText>私の前だから意地張ってるけど
本当は読みたいに決まってる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <BubbleId>4</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263524</PointerOffset>
      <JapaneseText>そっか。
んじゃあ、預かるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>263808</PointerOffset>
      <JapaneseText>お願いね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264556</PointerOffset>
      <JapaneseText>どうしてあの人、
素直になれないのかしら……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>22</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264556</PointerOffset>
      <JapaneseText>本当はお父さんのこと、
好きでしょうがないくせに……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>22</Id>
      <BubbleId>2</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>264556</PointerOffset>
      <JapaneseText>……いつものことか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>22</Id>
      <BubbleId>3</BubbleId>
      <Status>To Do</Status>
    </Entry>
  </Strings>
</SceneText>
