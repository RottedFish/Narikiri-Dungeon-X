    <Entry>
      <PointerOffset>37244</PointerOffset>
      <VoiceId>VCT_003_093_001</VoiceId>
      <JapaneseText>&lt;Mel&gt;、さっき……何があったの？
あの地下の通路で……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37644</PointerOffset>
      <VoiceId>VCT_003_093_002</VoiceId>
      <JapaneseText>うん……でも今は自分でも
まだよくわかってないんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38380</PointerOffset>
      <VoiceId>VCT_003_093_003</VoiceId>
      <JapaneseText>わかったら、ちゃんと話すね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38756</PointerOffset>
      <VoiceId>VCT_003_093_004</VoiceId>
      <JapaneseText>……そっか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39200</PointerOffset>
      <VoiceId>VCT_003_093_005</VoiceId>
      <JapaneseText>………………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
