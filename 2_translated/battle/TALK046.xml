    <Entry>
      <PointerOffset>4</PointerOffset>
      <VoiceId>VBE01_48_01</VoiceId>
      <JapaneseText>おかえりなさい、&lt;Couleur&gt;</JapaneseText>
      <EnglishText>Welcome home, &lt;Couleur&gt;!</EnglishText>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>20</PointerOffset>
      <VoiceId>VBE01_48_02</VoiceId>
      <JapaneseText>準備はいいか、&lt;Couleur&gt;</JapaneseText>
      <EnglishText>Get ready to fight, &lt;Couleur&gt;!</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>36</PointerOffset>
      <VoiceId>VBE01_48_03</VoiceId>
      <JapaneseText>クルー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>52</PointerOffset>
      <VoiceId>VBE01_48_04</VoiceId>
      <JapaneseText>私の強さ、
あなたが一番知っているはず……</JapaneseText>
      <EnglishText>There is no one who knows my power more than
you, yet-</EnglishText>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>4</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>68</PointerOffset>
      <VoiceId>VBE01_48_05</VoiceId>
      <JapaneseText>クルール！</JapaneseText>
      <EnglishText>Kuruuru!</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>84</PointerOffset>
      <VoiceId>VBE01_48_06</VoiceId>
      <JapaneseText>いいでしょう！　ともに来なさい！</JapaneseText>
      <EnglishText>I see. Then come!</EnglishText>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>6</Id>
      <Status>Editing</Status>
    </Entry>
