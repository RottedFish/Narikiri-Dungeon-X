    <Entry>
      <PointerOffset>4</PointerOffset>
      <VoiceId>VBE01_10_01</VoiceId>
      <JapaneseText>覚悟するのだな！</JapaneseText>
      <EnglishText>Ready your weapons!</EnglishText>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>20</PointerOffset>
      <VoiceId>VBE01_10_02</VoiceId>
      <JapaneseText>お願い、話を聞いて……</JapaneseText>
      <EnglishText>Please! You have to listen to us!</EnglishText>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>36</PointerOffset>
      <VoiceId>VBE01_10_03</VoiceId>
      <JapaneseText>聞かぬ！　問答無用じゃ！</JapaneseText>
      <EnglishText>I refuse to take part in fruitless arguments!</EnglishText>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>52</PointerOffset>
      <VoiceId>VBE01_10_04</VoiceId>
      <JapaneseText>言っても分かってくんねーって</JapaneseText>
      <EnglishText>You have to listen!</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>68</PointerOffset>
      <VoiceId>VBE01_10_05</VoiceId>
      <JapaneseText>にしてもここまで嫌われてるのって……</JapaneseText>
      <EnglishText>I've no idea why you hate us all of a sudden...</EnglishText>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>84</PointerOffset>
      <VoiceId>VBE01_10_06</VoiceId>
      <JapaneseText>いや……これは……</JapaneseText>
      <EnglishText>No, please...this is...</EnglishText>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>6</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>100</PointerOffset>
      <VoiceId>VBE01_10_07</VoiceId>
      <JapaneseText>気をつけて、来るよ！</JapaneseText>
      <EnglishText>Be careful! Here they come!</EnglishText>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>7</Id>
      <Status>Editing</Status>
    </Entry>
